PostProcess {
  FarField {
    FieldBagPath = "%(field)s"
    OutputFileName = "first%(oPath)s"
    Rotation = X:Z:-Y
    Polar {
      Radius = %(radius)e
      GridPointsTheta = %(points_the)e
      GridPointsPhi = %(points_phi)e
    }
  }
}
PostProcess {
  FarField {
    FieldBagPath = "%(circfield)s"
    OutputFileName = "second%(oPath)s"
    Rotation = X:Z:-Y
    Polar {
      Radius = %(radius)e
      GridPointsTheta = %(points_the)e
      GridPointsPhi = %(points_phi)e
    }
  }
}
